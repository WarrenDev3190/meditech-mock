<div class="row">
	<div class="col s12">
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">account_circle</i>
				<input type="text" id="patient_name" class="validate">
				<label for="patient_name">Patient Name</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">schedule</i>
				<input type="text" id="occured" class="validate">
				<label for="occured">Occured</label>
			</div>
			<div class="input-field col s6">
				<i class="material-icons prefix">subtitles</i>
				<input type="text" id="at_terminal" class="validate">
				<label for="at_terminal">Recorded At Terminal</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">schedule</i>
				<input type="text" id="recorded" class="validate">
				<label for="recorded">Recorded</label>
			</div>
			<div class="input-field col s6">
				<i class="material-icons prefix">supervisor_account</i>
				<input type="text" id="by_monitor" class="validate">
				<label for="by_monitor">By Monitor</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">av_timer</i>
				<input type="text" id="urinary" class="validate">
				<label for="urinary">Indication for urinary catheter today.</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">invert_colors</i>
				<input type="text" id="symptoms" class="validate">
				<label for="symptoms">Urinary Symptoms.</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">thumbs_up_down</i>
				<input type="text" id="foley" class="validate">
				<label for="foley">Daily Foley care performed.</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s6">
				<i class="material-icons prefix">error</i>
				<input type="text" id="complication" class="validate">
				<label for="complication">Complication encountered in daily care.</label>
			</div>
		</div>
		<div class="row">
			<div class="input-field col s12">
			  <i class="material-icons prefix">comment</i>
	          <textarea id="comments" class="materialize-textarea"></textarea>
	          <label for="comments">Additional Comments</label>
	        </div>
		</div>
